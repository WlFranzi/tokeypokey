
    

<%= form_for(@trip) do |f| %>
  <%= f.label :name %>
    <%= f.text_field :name %>
<br>
    <%= f.label :duration %>
    <%= f.text_field :duration %>
<br>
     <%= f.label :continent %>
    <%= f.text_field :continent %>
<br>
     <%= f.label :country %>
    <%= f.text_field :country %>
<br>
     <%= f.label :photo %>
    <%= f.text_field :photo %>
<br>

  <%= f.submit %>

<% end %>
